	<cache name="gov.nih.nci.objectCart.domain.CartObject"
		maxElementsInMemory="500"
		eternal="false"
		timeToIdleSeconds="1800"
		timeToLiveSeconds="100000"
		overflowToDisk="true"/>
	
</ehcache>